      <%= render partial: "tags/tag", collection: most_week %>
  </ul>
</div>

<div class="tags">
	<h4 class="tag_head"><%= t "global.month_tags" %></h4>
	<ul class="tags_links">
      <%= render partial: "tags/tag", collection: most_month %>
  </ul>
  <% end %>
</div>